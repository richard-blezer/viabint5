                    "val": false
                }
            },
            "manufacturerfilter": {
                "label": "Manufactureres Filter",
                "key": "_#_platform_#_.filter.manufacturer",
                "type": "multicheckbox",
                "desc": "All selected manufacturers will be excluded in the product preparation view.",
                "default": [],
                "expertsetting": true
            },
            "lang": {
                "label": "Articledescription",
                "key": "_#_platform_#_.lang",
                "type": "selection",
                "values": null
            }
        }
    },
    "matchingvalues": {
        "headline": "",
