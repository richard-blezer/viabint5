    "apply": {
        "headline": "",
        "desc": "<b>Create New Products<\/b>",
        "fields": {
            "mfn": {
                "label": "Alternative Manufacturer",
                "desc": "If a item is not saved in combination with a manufacturer, this one will be used alternatively.",
                "key": "amazon.prepare.manufacturerfallback",
                "type": "text"
            },
            "mfnpartno": {
                "label": "Manufacturer Part Number",
                "desc": "If you use a contribution to manage the manufacturer part number in a special database field, you can select the used database table and column here. By this setting, the proper field will be prefilled during the item preparing process. <br \/><br \/>If the column \"products_id\" uses another field name, please enter the alternative description of the used column. <br \/><br \/>Hint: These settings will not be verified. If they are erroneous, database errors will occur!",
                "key": "amazon.prepare.manufacturerpartnumber.table",
                "type": "dbfieldselector",
                "expertsetting": true,
                "morefields": {
                    "alias": {
                        "label": "<span class=\"tt\">`products_id`<\/span> Alias",
                        "key": "amazon.prepare.manufacturerpartnumber.alias",
                        "type": "text",
                        "cssClasses": ["autoWidth"]
                    }
                }
            },
            "imagepath": {
                "label": "Image-Path",
                "key": "amazon.imagepath",
                "type": "text",
                "expertsetting": true,
                "desc": "Directory, where the product images will be taken."
            }
        }
    },
    "1": {
        "headline": "V.A.T.",
        "fields": [{
            "label": "Conditions Note",
            "desc": "Amazon does not display the V.A.T. in some cases.\n\t\t\t\t\tThe conditions note is the only variable field to make a hint to the V.A.T.<br \/>\n\t\t\t\t\tYou can adjust the text, with the exception of ##. V.A.T.: It will be set automatically by the system.",
            "hint": "Wildcard: ## replaces V.A.T. automatically.",
            "key": "amazon.mwstplaceholder",
            "type": "text",
            "default": "Price incl. ##% V.A.T.",
            "verify": "contains(\"##\")"
        }, {
            "label": "V.A.T. for Items, not Listed in the Shop",
            "desc": "If the item is not listed by magnalister, V.A.T. can not be determined.<br\/>\n\t\t\t\t\tAs a solution, the V.A.T. value added here, will be inserted for all items, that are not listed in the shop.",
            "hint": "V.A.T. in percentage: Inserted for Amazon-items, that are not listed in the shop.",
            "key": "amazon.mwst.fallback",
            "type": "text",
            "default": "19",
            "verify": "float"
        }, {
            "label": "V.A.T. Shipping Costs",
            "desc": "Amazon does not submit the V.A.T by the API. Due to this reason, you can enter the V.A.T. for shipping costs in this field.",
            "hint": "V.A.T for shipping costs %.",
            "key": "amazon.mwst.shipping",
            "type": "text",
            "default": "19",
            "verify": "float"
        }]
    },
    "stockCI": {
        "headline": "Listing Process",
        "fields": {
            "0": {
                "label": "Status Filter",
                "key": "amazon.checkin.status",
                "type": "checkbox",
                "values": {
                    "val": "Display only active items"
                },
                "default": {
                    "val": false
                }
            },
            "quantity": {
                "label": "Stock of Inventory",
                "desc": "Please set the amount of available items on Amazon. \n\t\t\t\t\tYou can adjust the amount during the listing process individually.",
                "key": "amazon.quantity.type",
                "type": "selection",
                "values": {
                    "stock": "Take stock of the shop",
                    "stocksub": "Take stock of the shop minus value from right field",
                    "lump": "General (from right field)"
                },
                "default": "lump",
                "morefields": {
                    "quantity": {
                        "key": "amazon.quantity.value",
                        "type": "text",
                        "default": 1,
                        "verify": "int",
                        "cssClasses": ["autoWidth"]
                    }
                }
            },
            "manufacturerpartnumber": {
                "label": "Manufacturer-Part-Number",
                "desc": "If no Manufacturer-Part-Number is specified the SKU will be used as Manufacturer-Part-Number.",
                "key": "_#_platform_#_.checkin.SkuAsMfrPartNo",
                "type": "checkbox",
                "values": {
                    "val": "Use the SKU as Manufacturer-Part-Number"
                },
                "default": {
                    "val": true
                }
            },
            "leadtimetoship": {
                "label": "Handling Time (in Days)",
                "desc": "Sets the handling time between order date and shipping date (in days).<br \/><br \/>If you do not enter any settings, default will be 1-2 days.<br \/><br \/>\n                    <table width=\"100%\" cellspacing=\"0\" cellpadding=\"6\" bordercolor=\"#DDDDDD\" border=\"1\">\n                        <tbody>\n                            <tr valign=\"center\">\n                                <td bgcolor=\"#F8F5D8\"><strong>Input<\/strong><\/td>\n                                <td bgcolor=\"#F8F5D8\"><strong>Availability Message<\/strong><\/td>\n                                <td bgcolor=\"#F8F5D8\"><strong>Min Days<\/strong><\/td>\n                                <td bgcolor=\"#F8F5D8\"><strong>Max Days<\/strong><\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">NULL,&nbsp;0<\/td>\n                                <td>Usually ships in 1-2 business days<\/td>\n                                <td>1<\/td>\n                                <td valign=\"top\">2<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">1<\/td>\n                                <td>*Currently 1 to 2, but may be changed to 1 business day or 24 hours<\/td>\n                                <td>1<\/td>\n                                <td valign=\"top\">2<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">2<\/td>\n                                <td>Usually ships in 1-2 business days<\/td>\n                                <td>1<\/td>\n                                <td valign=\"top\">2<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">3<\/td>\n                                <td>Usually ships in 2-3 business days<\/td>\n                                <td>2<\/td>\n                                <td valign=\"top\">3<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">4<\/td>\n                                <td>Usually ships in 3-4 business days<\/td>\n                                <td>3<\/td>\n                                <td valign=\"top\">4<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">5<\/td>\n                                <td>Usually ships in 4-5 business days<\/td>\n                                <td>4<\/td>\n                                <td valign=\"top\">5<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">6-10<\/td>\n                                <td>Usually ships in 1 to 2 weeks<\/td>\n                                <td>6<\/td>\n                                <td valign=\"top\">10<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">11-15<\/td>\n                                <td>Usually ships in 2 to 3 weeks<\/td>\n                                <td>11<\/td>\n                                <td valign=\"top\">15<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">16-20<\/td>\n                                <td>Usually ships in 3 to 4 weeks<\/td>\n                                <td>16<\/td>\n                                <td valign=\"top\">20<\/td>\n                            <\/tr>\n                            <tr>\n                                <td valign=\"top\">20-30<\/td>\n                                <td>Usually ships in 1 to 2 months<\/td>\n                                <td>21<\/td>\n                                <td valign=\"top\">30<\/td>\n                            <\/tr>\n                        <\/tbody>\n                    <\/table>",
                "key": "amazon.leadtimetoship",
                "type": "selection",
                "values": {
                    "": "&mdash;",
                    "__calc__": "range(1,30)"
                }
            },
            "leadtimetoshipMatching": {
                "label": "Handling Time Matching",
                "key": "_#_platform_#_.leadtimetoshipmatching.values",
                "type": "extern",
                "cssClasses": ["valignmid"],
                "procFunc": "amazonLeadtimeToShipMatching",
                "params": [],
                "morefields": {
                    "quantity": {
                        "key": "_#_platform_#_.leadtimetoshipmatching.prefer",
                        "type": "checkbox",
                        "values": {
                            "val": "Prefer Handling Time Matching"
                        },
                        "default": {
                            "val": false
                        }
                    }
                }
            }
        }
    },
    "price": {
        "headline": "Price Calculation",
        "fields": {
            "price": {
                "label": "Price",
                "desc": "You can enter a price surcharge or discount. Define a fix or percentel value. Please enter a minus (-) sign for a discount.",
                "key": "amazon.price.addkind",
                "type": "selection",
                "values": {
                    "percent": "x% surcharge or discount",
                    "addition": "x fix surcharge or discount"
                },
                "default": "percent",
                "morefields": {
                    "cost": {
                        "label": "Wert",
                        "key": "amazon.price.factor",
                        "type": "text",
                        "default": "0",
                        "verify": "float",
                        "cssClasses": ["autoWidth"],
                        "formatstr": "%.2f"
                    },
                    "signaluse": {
                        "label": "Threshold Price",
                        "desc": "The threshold price replaces the decimal place of your price.<br\/><br\/>\n\t\t\t\t\t\t           <strong>For Example:<\/strong> <br \/>\n\t\t\t\t\t\t\t\t   Value in textfield: 99 <br \/>\n\t\t\t\t\t\t\t\t   Price before: 5.58 <br \/>\n\t\t\t\t\t\t\t\t   Price after: 5.99 <br \/>\n\t\t\t\t\t\t           Particularly this feature helps to calculate a proper threshold price, if you use the function \"x% surcharge or discount\" .<br\/>\n\t\t\t\t\t\t           Please leave the field empty, or enter a number of two digits.",
                        "key": "amazon.price.signal",
                        "type": "text",
                        "cssClasses": ["autoWidth"]
                    }
                }
            },
            "whichprice": {
                "label": "Price Options",
                "leftlabel": "Price from customer group",
                "key": "amazon.price.group",
                "type": "selection",
                "values": null,
                "cssClasses": ["rightSpacer"],
                "morefields": {
                    "cost": {
                        "key": "amazon.price.usespecialoffer",
                        "type": "checkbox",
                        "values": {
                            "val": "use special pricing"
                        },
                        "default": {
                            "val": false
                        }
                    }
                }
            },
            "exchangerate": {
                "label": "Exchange Rate",
                "desc": "If the shop currency differs from the marketplace currency, the price will be automatically converted by the exchange rate from\n\t\t\t\t           Yahoo-Finances.<br \/><br \/>\n\t\t\t\t           <b>Liability Note:<\/b> RedGecko GmbH assumes no liability for the completeness or correctness of the exchange rate. Please \n\t\t\t\t\t\t   check your prices at your seller central account, after submitting.",
                "key": "amazon.exchangerate",
                "type": "checkbox",
                "values": {
                    "update": "Update exchange rate automatically"
                },
                "default": {
                    "update": true
                }
            }
        }
    },
    "import": {
        "headline": "Orders Import",
        "fields": {
            "0": {
                "label": "Activate Import",
                "desc": "Should orders from Amazon be imported? <br\/><br\/>\n\t\t\t\t           Orders are imported automatically every hour as the default setting.<br\/><br\/>\n\t\t\t\t           You can adjust the import-time individually, by configuring <br\/>\n\t\t\t\t           \"magnalister admin\" &rarr; \"Global Configuration\" &rarr; \"Orders Import\".<br\/><br\/>\n\t\t\t\t           You can manually import orders, by clicking the assigned button in the magnalister-header, next left to the ant-logo.<br\/><br\/>\n\t\t\t\t           Additionally, you can call order imports by setting a own cronjob to your following shop-link:<br\/>\n\t\t\t\t           <i>http:\/\/www.YourShop.com\/plugins\/magnalister\/magnaCallback.phpmagnaCallback.php?do=ImportOrders<\/i><br\/><br\/>\n\t\t\t\t           Setting an own cronjob is permitted for customers within the service plan \"Premium\", only.<br\/><br\/>\n\t\t\t\t           Own cronjob-calls, exceeding a quarter of an hour, or calls from customers, who are not within the service plan \"Premium\", will be blocked.\t\t\t\t           \n\t\t\t\t           ",
                "key": "amazon.import",
                "type": "radio",
                "values": {
                    "true": "Yes",
                    "false": "No"
                },
                "default": "true",
                "submit": "Orders.Import",
                "morefields": {
                    "preimportsince": {
                        "label": "first-time order-import",
                        "desc": "The date, an order import will be processed the first time.",
                        "key": "amazon.preimport.start",
                        "type": "date",
                        "default": "now",
                        "cssClasses": ["autoWidth", "rightSpacer"],
                        "submit": "Orders.Import.TS"
                    }
                }
            },
            "shop": {
                "label": "Client-Store",
                "desc": "Assigns the order to a client store.",
                "key": "amazon.shopmandant",
                "type": "selection",
                "values": null
            },
            "customersgroup": {
                "label": "Customer Groups",
                "desc": "Allocate customers from Amazon to a customer group of the shop.",
                "key": "amazon.CustomerGroup",
                "type": "selection",
                "values": null,
                "default": "2"
            },
            "openstatus": {
                "label": "Order Status",
                "desc": "Status set for an imported Amazon order.",
                "key": "amazon.orderstatus.open",
                "type": "selection",
                "values": null,
                "default": "2"
            },
            "defaultshipping": {
                "label": "Shipping method",
                "desc": "Default shipping method for _#_platformName_#_ orders.<br \/><br \/>\r\n\t\t\t\t _#_platformName_#_ does not provide a shipping method along with the order data, therefore we use the shipping method you choose here.",
                "key": "_#_platform_#_.orderimport.shippingmethod",
                "type": "selection",
                "values": {
                    "__ml_lump": "From Textfield"
                },
                "default": "__ml_lump",
                "expertsetting": true,
                "morefields": {
                    "quantity": {
                        "key": "_#_platform_#_.orderimport.shippingmethod.name",
                        "type": "text",
                        "default": "amazon",
                        "cssClasses": ["autoWidth"]
                    }
                }
            },
            "defaultpayment": {
                "label": "Payment Method of Orders",
                "desc": "Payment method for _#_platformName_#_ orders.<br\/ ><br \/>\r\n\t\t\t\t\t\t\tChoose 'Automatic matching' to use the payment method which the buyer has chosen on _#_platformName_#_.\r\n\t\t\t\t\t\t\tChoose a certain payment method, if you want that all of the _#_platformName_#_ orders get this method within your online shop.<br \/><br \/>\r\n\t\t\t\t\t\t\tThis setting influences the printing of shipping notes and bills and the further processing of orders within the shop. It can also be relevant for some ERPs.",
                "key": "_#_platform_#_.orderimport.paymentmethod",
                "type": "selection",
                "values": {
                    "textfield": "From Textfield",
                    "matching": "Automatic matching"
                },
                "default": "matching",
                "expertsetting": true,
                "morefields": {
                    "quantity": {
                        "key": "_#_platform_#_.orderimport.paymentmethod.name",
                        "type": "text",
                        "default": "amazon",
                        "cssClasses": ["autoWidth"]
                    }
                }
