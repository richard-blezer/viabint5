    "0": {
        "headline": "",
        "desc": "<b>Matching Behavior<\/b>",
        "fields": [{
            "label": "New matching",
            "desc": "If you select the checkbox, items that already have been matched, will be overwritten by a new matching.",
            "key": "amazon.multimatching",
            "type": "checkbox",
            "values": {
                "rematch": "Already matched items will be overwritten by multi- oder auto-matching."
            },
            "default": {
                "rematch": true
            }
        }, {
            "label": "Results",
            "desc": "Please define the amount of results, that are displayed in a page. <br\/>\n\t\t\t\t\tThe higher the value, the longer the loading-time (50 results approximately 30 seconds).",
            "rightlabel": "per Site (multi-matching)",
