    "shipping": {
        "headline": "",
        "desc": "<b>Einstellungen f&uuml;r Versand<\/b>",
        "fields": {
            "shippingcostlocal2": {
                "label": "Versand Inland",
                "desc": "Mindestens eine oder mehrere Versandarten ausw&auml;hlen, die standardm&auml;&szlig;ig verwendet werden soll.<br \/><br \/>Bei den Versandkosten k&ouml;nnen Sie eine Zahl eintragen (ohne Angabe der W&auml;hrung) oder \"=GEWICHT\", um die Versandkosten gleich dem Artikelgewicht zu setzen.",
                "type": "extern",
                "key": "ebay.default.shipping.local",
                "procFunc": "eBayShippingConfig",
                "params": []
            },
            "shippingprofilelocal": {
                "label": "Rabatte Kombizahlung und Versand",
                "desc": "Auswahl des Profils f&uuml;r Versandrabatte. Die Profile k&ouml;nnen Sie anlegen in Ihrem eBay account, unter Mein eBay -> Mitgliedskonto -> Einstellungen -> Versandeinstellungen<br \/><br \/>\n\t\t\t\tDie Regeln f&uuml;r Versand zum Sonderpreis (z.B. maximalen Versandpreis pro Bestellung, oder einen Betrag ab dem der Versand kostenfrei ist) k&ouml;nnen Sie ebenfalls dort anlegen.<br \/><br \/>\n<b>Hinweis:<\/b><br \/>\r\nBeim Bestell-Import wird die Regel verwendet, welche aktuell hier ausgew&auml;hlt ist (da wir von eBay nicht die Information bekommen, wie es beim Einstellen des Artikels ausgesehen hat).",
                "type": "selection",
                "key": "ebay.default.shippingprofile.local",
                "morefields": {
                    "promodiscount": {
                        "key": "ebay.shippingdiscount.local",
                        "type": "checkbox",
                        "values": {
                            "val": "Regeln f&uuml;r Versand zum Sonderpreis anwenden"
                        },
                        "default": {
                            "val": false
                        }
                    }
                }
            },
            "shippingcostinternational2": {
                "label": "Versand Ausland (optional)",
                "desc": "Keine oder mehrere Versandarten ausw&auml;hlen, die standardm&auml;&szlig;ig verwendet werden soll.",
                "type": "extern",
                "key": "ebay.default.shipping.international",
                "procFunc": "eBayShippingConfig",
                "params": []
            },
            "shippingprofileinternational": {
                "label": "Rabatte Kombizahlung und Versand",
                "desc": "Auswahl des Profils f&uuml;r Versandrabatte. Die Profile k&ouml;nnen Sie anlegen in Ihrem eBay account, unter Mein eBay -> Mitgliedskonto -> Einstellungen -> Versandeinstellungen<br \/><br \/>\n\t\t\t\tDie Regeln f&uuml;r Versand zum Sonderpreis (z.B. maximalen Versandpreis pro Bestellung, oder einen Betrag ab dem der Versand kostenfrei ist) k&ouml;nnen Sie ebenfalls dort anlegen.<br \/><br \/>\n<b>Hinweis:<\/b><br \/>\r\nBeim Bestell-Import wird die Regel verwendet, welche aktuell hier ausgew&auml;hlt ist (da wir von eBay nicht die Information bekommen, wie es beim Einstellen des Artikels ausgesehen hat).",
                "type": "selection",
                "key": "ebay.default.shippingprofile.international",
                "morefields": {
                    "promodiscount": {
                        "key": "ebay.shippingdiscount.international",
                        "type": "checkbox",
                        "values": {
                            "val": "Regeln f&uuml;r Versand zum Sonderpreis anwenden"
                        },
                        "default": {
                            "val": false
						}
					}
				}
			}
		}
    },
